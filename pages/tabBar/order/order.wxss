/* pages/tabBar/order/order.wxss */

.tab-h-view {
  width: 100%;
  height: 92rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #dedede;
  position: fixed;
  top: 0;
  left: 0;
}

.tab-item {
  display: inline-block;
  width: 14%;
  color: #777;
  font-size: 28rpx;
  line-height: 90rpx;
  text-align: center;
  margin: 0rpx 22rpx;
  box-sizing: border-box;
}

.tab-item.active {
  color: #f90;
  border-bottom: 4rpx solid #f90;
}
.main-item-view {
  padding-top: 92rpx;
}
.item {
  padding: 0 50rpx;
  background-color: #fff;
}

.item > view {
  padding-bottom: 10rpx;
}

.car-menoy-view view {
  line-height: 90rpx;
}

.car-menoy-view view:first-child {
  color: #333;
  font-size: 34rpx;
}

.car-menoy-view view:last-child {
  color: #fff;
  font-size: 18rpx;
  line-height: 18rpx;
  padding: 8rpx 12rpx;
  margin-top: 32rpx;
  border-radius: 6rpx;
  background-color: #f60;
}

.item image {
  width: 38rpx;
  height: 38rpx;
  padding: 0 10rpx 10rpx 0;
  vertical-align: middle;
}

.go-time-view > view {
  width: 100%;
}

.go-time-view > view:last-child {
  font-size: 26rpx;
}

.setAddress {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.nodata-view {
  text-align: center;
  margin-top: 300rpx;
}

.nodata image {
  width: 128rpx;
  height: 128rpx;
}
